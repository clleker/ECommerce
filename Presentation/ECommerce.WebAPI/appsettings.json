{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Data": {
    "MainDbContext": {
      "Provider": "SqlServer",
      "ConnectionString": "Server=.\\SQLEXPRESS; Database=ECommerce; Encrypt=false; Integrated Security=True;",
      "MigrationsAssembly": "ECommerce.Persistence",
      "DefaultTimeOut": 30,
      "ConnectionPoolSize": 1024
    }
  },

  "MessageBrokerOptions": {
    "HostName": "localhost",
    "UserName": "guest",
    "Password": "guest"
  },

  "CustomerTokenOptions": {
    "Audience": "www.clleker.com",
    "Issuer": "www.clleker.com",
    "AccessTokenExpiration": 1000,
    "SecurityKey": "mysecretkeymysecretkey"
  },
  "AllowedHosts": "*"
}
